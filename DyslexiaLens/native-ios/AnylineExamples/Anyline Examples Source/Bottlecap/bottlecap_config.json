{
    "captureResolution":"1080",
    "cutout": {
        "style": "rect",
        "maxWidthPercent": "100%",
        "maxHeightPercent": "100%",
        "alignment": "center",
        "width": 1080,
        "ratioFromSize": {
            "width": 6,
            "height": 1
        },
        "strokeWidth": 2,
        "cornerRadius": 3,
        "strokeColor": "FFFFFF",
        "outerColor": "000000",
        "outerAlpha": 0.3
    },
    "flash": {
        "mode": "manual",
        "alignment": "top_left"
    },
    "visualFeedback": {
        "style": "rect",
        "strokeWidth": 3,
        "strokeColor": "8800FF00",
        "fillColor": "3300FF00",
        "cornerRadius": 6
        
    },
    "beepOnResult": false,
    "vibrateOnResult": false,
    "blinkAnimationOnResult": false,
    "cancelOnResult": false
}
